{
  "name": "Gmail Scheduler",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://mail.google.com/*", "https://mail.google.com/*"],
      "css": ["style.css"],
      "js": ["gmail.js"]
    }
  ],
  "web_accessible_resources": [
    "icon.png"
  ],
  "icons": {
    "16" : "icon.png",
    "48" : "icon.png",
    "128" : "icon.png"
  },
  "manifest_version": 2
}
